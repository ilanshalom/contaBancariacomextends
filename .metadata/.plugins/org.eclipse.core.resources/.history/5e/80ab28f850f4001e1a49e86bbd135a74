package application;

import entities.Account;
import entities.BusinessAccount;
import entities.SavingsAccount;


public class Program {

	public static void main(String[] args) {
    
		Account acc1 = new Account(1001, "ILan", 1000.0); 
		acc1.withdraw(800.0);
		System.out.println(acc1.getBalace());
		
		Account acc2 = new SavingsAccount(1002,"luis", 1000.0, 0.01);
        acc2.withdraw(200.0);
        System.out.println(acc2.getBalace());
        
        Account acc3 = new BusinessAccount(1003,"Davi", 1000.0, 500.0);
        acc3.withdraw(200.0);
        System.out.println(acc3.getBalace());
 }
}